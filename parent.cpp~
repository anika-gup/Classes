using namespace std;

parent::parent() {
  cout << "Parent CPP !!" << endl;
  year=0;
  title=" ";
  

}

void setTitle(char myTitle[50]) {
  title = myTitle;

}
char* getTitle() {
  return title;
}
void setYear(int Year) {
  year = Year;
}
